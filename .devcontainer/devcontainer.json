{
    "name": "Flutter Codespace",
      "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "ghcr.io/devcontainers/features/java:1": { "version": "17" }
              },
                "postCreateCommand": "bash .devcontainer/install_flutter.sh",
                  "customizations": {
                      "vscode": {
                            "extensions": [
                                    "Dart-Code.dart-code",
                                            "Dart-Code.flutter"
                                                  ]
                                                      }
                                                        }
                                                        }
